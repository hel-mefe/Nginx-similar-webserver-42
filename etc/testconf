http
{
    allowed_methods GET DELETE POST
    max_body_size 0

    server
    {
        server_name nice_server
        port 8080
		root /www
        error_page 404 404.html
        location /error_pages
        {
            directory_listing on
        }
        
        location /testing
        {
            directory_listing on
            index index.oho index.php index.html
        }
    }
}
