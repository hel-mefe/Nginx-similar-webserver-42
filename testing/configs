http
{
  allowed_methods GET POST DELETE
  max_request_timeout 2
  multiplexer poll

  server
  {
    port 8080
    server_name server
    
    error_page 404 error_pages/404.html
    error_page 403 error_pages/403.html
    # fastCGI .pl /usr/bin/perl
    # fastCGI .py /usr/bin/python
    # fastCGI .php /Users/hel-mefe/Desktop/webserverPollClean42/cgi-bin/php-cgi

    index index.html

    # comment 1

    location /listing
    {
      directory_listing on
    }

    # comment 2

    location /testing/indexes
    {
      index index1.html index2.html index3.html
    }
    #comment 3
  }

}
